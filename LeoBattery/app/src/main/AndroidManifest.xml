<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.leo.forest.digital">
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_digital_wellbeing"
        android:label="@string/app_name"
        android:theme="@style/ThemeLeo"
        android:supportsRtl="true">
        <activity
            android:name="org.leo.forest.digital.MainActivity"
            android:theme="@style/ThemeLeo"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <intent-filter android:priority="6">
                <action android:name="com.android.settings.MANUFACTURER_APPLICATION_SETTING"/>
            </intent-filter>
            <meta-data android:name="com.samsung.android.settings.menu.group" android:value="group2"/>
            <meta-data android:name="com.android.settings.category" android:value="com.android.settings.category.ia.homepage"/>
            <meta-data android:name="com.android.settings.tileid" android:value="digital_wellbeing_settings"/>
            <meta-data android:name="com.android.settings.support" android:value="Common"/>
            <meta-data android:name="com.android.settings.title" android:resource="@string/app_name"/>
            <meta-data android:name="com.android.settings.summary" android:resource="@string/app_name_sumarry"/>
            <meta-data android:name="com.android.settings.icon" android:resource="@drawable/ic_digital_wellbeing"/>
        </activity>
    </application>
</manifest>